fabric {
  plugin_versions = {
    "blackstork/nist_nvd" = "<0.1.0"
  }

  plugin_registry {
    # Fabric is executed from `dist` dir
    mirror_dir = "./plugins/"
  }
}


document "ubertemplate" {

  data rss "blackstork_blog" {
    url = "https://blackstork.io/blog/index.xml"
  }

  data nist_nvd_cves "cves" {
    limit = 1
  }
}
